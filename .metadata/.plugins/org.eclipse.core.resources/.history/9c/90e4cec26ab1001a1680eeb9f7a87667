package com.srm.thread.creation;

public class SavingsAccountLoaderByThreadClass extends Thread{  
	
	public void run(){  
		
		System.out.println("Savings Account Loader Started ======> ");  
		
	}  
	
	public static void main(String args[]){  
		SavingsAccountLoaderByThreadClass obj =new SavingsAccountLoaderByThreadClass();  
		obj.start();  
	}  

}
