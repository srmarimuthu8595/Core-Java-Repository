package com.srm.collections.queues;

import java.util.Comparator;

public class BankCustomerDetails2 implements Comparator<BankCustomerDetails2> {  
	
	
	
	private String name;
    private int age;
    private int income;
 
    public BankCustomerDetails2(String name, int age, int income) {
        this.name = name;
        this.age = age;
        this.income = income;
    }
   
    public String toString() {
        return this.name + "\t" + this.age + "\t" + this.income;
    }
 
	@Override
	public int compare(BankCustomerDetails2 arg0, BankCustomerDetails2 arg1) {
		
		BankCustomerDetails2 cust1=(BankCustomerDetails2)arg0;  
		BankCustomerDetails2 cust2=(BankCustomerDetails2)arg1;  
  	  
    	if(cust1.income==cust1.income)  
    	return 0;  
    	else if(cust1.income>cust2.income)  
    	return 1;  
    	else  
    	return -1;  
    	
	}
    

}
