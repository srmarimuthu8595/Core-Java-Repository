package com.srm.thread.lifecycle;

public class ThreadLifeCycleTester {
	
	 public static void main(String args[]) {
		
		 ThreadLifeCycle thread1 = new ThreadLifeCycle( "Thread - One");
		 thread1.start();
	      
	      ThreadLifeCycle R2 = new ThreadLifeCycle( "Thread- Two");
	      R2.start();
	  
	 }   

}
