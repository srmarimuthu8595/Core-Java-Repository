package com.srm.thread.synchronization2;

import com.srm.thread.synchronization.SharedResource;

public class CurrentAccountThread extends Thread{ 
	
	AccountDetailsIntoTextFileWriter txtWriter;
	
	CurrentAccountThread(AccountDetailsIntoTextFileWriter shrdResource){  
		this.shrdResource=shrdResource;  
     }  
	
	
	public void run(){  
		shrdResource.sharedResourceOperation("Current Account Extractor");  
	}  
	
}