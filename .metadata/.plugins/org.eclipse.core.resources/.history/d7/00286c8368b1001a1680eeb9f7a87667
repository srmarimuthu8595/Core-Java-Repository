package com.srm.thread.creation;

public class SavingsAccountLoader extends Thread{  
	
	public void run(){  
		
		System.out.println("Savings Account Loader Started ======> ");  
		
	}  
	
	public static void main(String args[]){  
		SavingsAccountLoader obj =new SavingsAccountLoader();  
		obj.start();  
	}  

}
