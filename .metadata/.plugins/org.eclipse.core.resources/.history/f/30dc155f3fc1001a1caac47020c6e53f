package com.srm.object;

public class Staticvariables {
	
	//public static String name ="Chinnasamy";
	public static String name="";
	
	Staticvariables(){
		name="asdasdf";
	}

	public void printmyname(){
		name="asdasdf";
	}
	
	
	
	
	
	public static void main(String[] args) {
	
		name="asdasdf";
	}

}
