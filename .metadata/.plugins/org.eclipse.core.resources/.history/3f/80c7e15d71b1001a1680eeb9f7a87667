package com.srm.thread.synchronization;

public class SharedResourceTester {
	
	public static void main(String args[]){  
		
		SharedResource shrdResource = new SharedResource();
		
		ExtractorThreadOne extractorOne =new ExtractorThreadOne(shrdResource);  
		ExtractorThreadTwo extractorTwo =new ExtractorThreadTwo(shrdResource);  
		
		extractorOne.start();  
		
		extractorTwo.start();  
	
	}  

}
