	    	System.out.println("Current Account Loader Starded");  
		}  
		  
		public static void main(String args[]){  
			
			CurrentAccountLoader obj =new CurrentAccountLoader();  
		    Thread t1 =new Thread(m1);  
		    t1.start();  
	}  

}  