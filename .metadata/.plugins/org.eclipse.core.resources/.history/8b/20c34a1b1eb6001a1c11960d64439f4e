package com.srm.spring.core;

public class BankEmployeeDetails {
	
	private int employeeId;  
	private String employeeName;  
	  

	  
	public BankEmployeeDetails(int id, String name) {  
	    this.id = id;  
	    this.name = name;  
	}  
	  
	void showBankEmployeeDetails(){  
	    System.out.println(id+" "+name);  
	}  
}
